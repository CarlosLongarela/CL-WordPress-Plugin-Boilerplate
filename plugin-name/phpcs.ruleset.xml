<?xml version="1.0"?>
<ruleset name="Cl_Woo_Dorset_Waivers">
	<description>WooCommerce Waivers plugin addon for Dorset Adventure Park.</description>

	<rule ref="WordPress-Core" />
	<rule ref="WordPress-Docs" />
	<rule ref="WordPress-Extra" />

	<rule ref="WordPress.WP.I18n">
		<properties>
			<property name="text_domain" type="array" value="cl-woo-dorset-waivers" />
		</properties>
	</rule>

	<exclude-pattern>*/node_modules/*</exclude-pattern>
	<exclude-pattern>*/vendor/*</exclude-pattern>
</ruleset>
